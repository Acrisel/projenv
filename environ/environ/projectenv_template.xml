<?xml version="1.0" ?>

<environment>
  <environ>
    <var name='AC_PROJ_PREFIX' value='${__AC_TEMPLATE_PREFIX__}' export='True'/>
    <var name='${__AC_TEMPLATE_PREFIX__}WS_LOC' value='${__AC_TEMPLATE_WS_LOC__}' override='True' export='True' cast='path'/> 
    <var name='${__AC_TEMPLATE_PREFIX__}PROJ_NAME' value='${__AC_TEMPLATE_PROJ_NAME__}' export='True'/>
    <var name='${__AC_TEMPLATE_PREFIX__}PROJ_VER' value='0.90.0' export='True'/>
    <var name='${__AC_TEMPLATE_PREFIX__}PROJ_LOC' value='${${__AC_TEMPLATE_PREFIX__}WS_LOC}${${__AC_TEMPLATE_PREFIX__}PROJ_NAME}/' export='True'/>
    <var name='${__AC_TEMPLATE_PREFIX__}PROJ_API_LOC' value='${${__AC_TEMPLATE_PREFIX__}PROJ_LOC}api/' export='True'/>
    <var name='${__AC_TEMPLATE_PREFIX__}PROJ_DB_LOC' value='${${__AC_TEMPLATE_PREFIX__}PROJ_LOC}db/' export='True'/>
    <var name='${__AC_TEMPLATE_PREFIX__}PROJ_SRC_LOC' value='${${__AC_TEMPLATE_PREFIX__}PROJ_LOC}src/' export='True'/>
    <var name='${__AC_TEMPLATE_PREFIX__}PROJ_REF_LOC' value='${${__AC_TEMPLATE_PREFIX__}PROJ_LOC}ref/' export='True'/>
    <var name='${__AC_TEMPLATE_PREFIX__}PROJ_FMT_LOC' value='${${__AC_TEMPLATE_PREFIX__}PROJ_LOC}fmt/' export='True'/>
    <var name='${__AC_TEMPLATE_PREFIX__}PROJ_RES_LOC' value='${${__AC_TEMPLATE_PREFIX__}PROJ_LOC}res/' export='True'/>
    
    <var name='${__AC_TEMPLATE_PREFIX__}ENCODING' value='ascii' export='True'/>
    <var name='${__AC_TEMPLATE_PREFIX__}BYTEORDER' value='network' export='True'/>
    <var name='${__AC_TEMPLATE_PREFIX__}PAGE_BYTES' value='2048' cast='integer' export='True'/>
    <var name='${__AC_TEMPLATE_PREFIX__}RUNNER_NAMEDPIPE' value='commander' export='True'/>
    
    <var name='${__AC_TEMPLATE_PREFIX__}VS_LOC' value='${__AC_TEMPLATE_VS_LOC__}' override='True' export='True'/>
    <var name='AC_ENV_NAME' value='.' override='True' export='True'/>
    <var name='${__AC_TEMPLATE_PREFIX__}VAR_LOC' value='${${__AC_TEMPLATE_PREFIX__}VS_LOC}${${__AC_TEMPLATE_PREFIX__}PROJ_NAME}/${AC_ENV_NAME}/' override='True' export='True'/>
    <var name='${__AC_TEMPLATE_PREFIX__}LOG_LOC' value='${${__AC_TEMPLATE_PREFIX__}VAR_LOC}log/' override='True' export='True'/>
    <var name='${__AC_TEMPLATE_PREFIX__}REJ_LOC' value='${${__AC_TEMPLATE_PREFIX__}VAR_LOC}rej/' override='True' export='True'/>
    <var name='${__AC_TEMPLATE_PREFIX__}RUN_LOC' value='${${__AC_TEMPLATE_PREFIX__}VAR_LOC}run/' override='True' export='True'/>
    <var name='${__AC_TEMPLATE_PREFIX__}IN_LOC' value='${${__AC_TEMPLATE_PREFIX__}VAR_LOC}in/' override='True' export='True'/>
    <var name='${__AC_TEMPLATE_PREFIX__}OUT_LOC' value='${${__AC_TEMPLATE_PREFIX__}VAR_LOC}out/' override='True' export='True'/>
    <var name='${__AC_TEMPLATE_PREFIX__}DB_LOC' value='${${__AC_TEMPLATE_PREFIX__}VAR_LOC}db/' override='True' export='True'/>
    <var name='${__AC_TEMPLATE_PREFIX__}LKUP_LOC' value='${${__AC_TEMPLATE_PREFIX__}VAR_LOC}lkup/' override='True' export='True'/>
    <var name='${__AC_TEMPLATE_PREFIX__}TMP_LOC' value='${${__AC_TEMPLATE_PREFIX__}VAR_LOC}tmp/' override='True' export='True'/>
    
    <!-- Logging controls -->
    <var name='AC_LOG_LEVEL' value='DEBUG' override='True' export='True'/>
    <var name='AC_LOG_STDOUT' value='False' override='True' export='True' cast='boolean'/>
    <var name='AC_LOG_STDOUT_LEVEL' value='INFO' override='True' export='True'/>
    <var name='AC_LOG_STDERR' value='True' override='True' export='True' cast='boolean'/>
    <var name='AC_LOG_STDERR_LEVEL' value='WARNING' override='True' export='True'/>
    
    <!-- Accord Private Vars -->
    <var name='AC_WORK_LOC_NAME' value='.work' override='True' export='True'/>
    <var name='AC_DUMPER_MAX_OPEN' value='10' override='True' export='True' cast='integer'/>
    
    <!-- Influence part behavior -->
    <var name='AC_REJECTS_ALLOWED' value='0' override='True' cast='integer' export='True'/>
    <var name='AC_USE_REJECT_PORT' value='True' override='True' cast='boolean' export='True'/>
    <var name='AC_ENGINE_DEFAULT' value='Process' override='True' cast='string' export='True'/>
  </environ>
</environment>