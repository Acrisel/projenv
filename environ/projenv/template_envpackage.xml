<?xml version="1.0" ?>

<environment>
  <environ>
    <var name='PROJ_PREFIX' value='${__TEMPLATE_PREFIX__}' export='True'/>
    <var name='${__TEMPLATE_PREFIX__}PROJ_NAME' value='${__TEMPLATE_PROJ_NAME__}' export='True'/>
    <var name='${__TEMPLATE_PREFIX__}PROJ_VER' value='${__TEMPLATE_VERSION__}' export='True'/>
    <var name='${__TEMPLATE_PREFIX__}PROJ_LOC' value='' export='True' virtual='True' override='True'/>
    <var name='${__TEMPLATE_PREFIX__}PROJ_API_LOC' value='${${__TEMPLATE_PREFIX__}PROJ_LOC}api/' export='True'/>
    <var name='${__TEMPLATE_PREFIX__}PROJ_DB_LOC' value='${${__TEMPLATE_PREFIX__}PROJ_LOC}db/' export='True'/>
    <var name='${__TEMPLATE_PREFIX__}PROJ_SRC_LOC' value='${${__TEMPLATE_PREFIX__}PROJ_LOC}src/' export='True'/>
    <var name='${__TEMPLATE_PREFIX__}PROJ_FMT_LOC' value='${${__TEMPLATE_PREFIX__}PROJ_LOC}fmt/' export='True'/>
    <var name='${__TEMPLATE_PREFIX__}PROJ_RES_LOC' value='${${__TEMPLATE_PREFIX__}PROJ_LOC}res/' export='True'/>
    <var name='${__TEMPLATE_PREFIX__}PROJ_CFG_LOC' value='${${__TEMPLATE_PREFIX__}PROJ_LOC}cfg/' export='True'/>
    <var name='${__TEMPLATE_PREFIX__}PROJ_DATA_LOC' value='${${__TEMPLATE_PREFIX__}PROJ_LOC}data/' export='True'/>
    
    <var name='${__TEMPLATE_PREFIX__}ENCODING' value='ascii' export='True'/>
    <var name='${__TEMPLATE_PREFIX__}BYTEORDER' value='network' export='True'/>
    
    <var name='${__TEMPLATE_PREFIX__}VS_LOC' value='${__TEMPLATE_VS_LOC__}' override='True' export='True'/>
    <var name='${__TEMPLATE_PREFIX__}ENV_NAME' value='.' override='True' export='True'/>
    <var name='${__TEMPLATE_PREFIX__}VAR_LOC' value='${${__TEMPLATE_PREFIX__}VS_LOC}${${__TEMPLATE_PREFIX__}PROJ_NAME}/${ENV_NAME}/' override='True' export='True'/>
    <var name='${__TEMPLATE_PREFIX__}LOG_LOC' value='${${__TEMPLATE_PREFIX__}VAR_LOC}log/' override='True' export='True'/>
    <var name='${__TEMPLATE_PREFIX__}REJ_LOC' value='${${__TEMPLATE_PREFIX__}VAR_LOC}rej/' override='True' export='True'/>
    <var name='${__TEMPLATE_PREFIX__}RUN_LOC' value='${${__TEMPLATE_PREFIX__}VAR_LOC}run/' override='True' export='True'/>
    <var name='${__TEMPLATE_PREFIX__}IN_LOC' value='${${__TEMPLATE_PREFIX__}VAR_LOC}in/' override='True' export='True'/>
    <var name='${__TEMPLATE_PREFIX__}OUT_LOC' value='${${__TEMPLATE_PREFIX__}VAR_LOC}out/' override='True' export='True'/>
    <var name='${__TEMPLATE_PREFIX__}DB_LOC' value='${${__TEMPLATE_PREFIX__}VAR_LOC}db/' override='True' export='True'/>
    <var name='${__TEMPLATE_PREFIX__}LKUP_LOC' value='${${__TEMPLATE_PREFIX__}VAR_LOC}lkup/' override='True' export='True'/>
    <var name='${__TEMPLATE_PREFIX__}TMP_LOC' value='${${__TEMPLATE_PREFIX__}VAR_LOC}tmp/' override='True' export='True'/>
    
    <!-- Logging controls -->
    <var name='${__TEMPLATE_PREFIX__}LOG_LEVEL' value='DEBUG' override='True' export='True'/>
    <var name='${__TEMPLATE_PREFIX__}LOG_STDOUT' value='False' override='True' export='True' cast='boolean'/>
    <var name='${__TEMPLATE_PREFIX__}LOG_STDOUT_LEVEL' value='INFO' override='True' export='True'/>
    <var name='${__TEMPLATE_PREFIX__}LOG_STDERR' value='True' override='True' export='True' cast='boolean'/>
    <var name='${__TEMPLATE_PREFIX__}LOG_STDERR_LEVEL' value='WARNING' override='True' export='True'/>
    
  </environ>
</environment>